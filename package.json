{
  "name": "tx-revert-reason",
  "version": "0.1.0",
  "license": "MIT",
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "pkgroll",
    "prepare": "husky install",
    "start": "npx esno src/cli"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write",
    "*.{js,ts}": "xo --fix",
    "package.json": "sort-package-json"
  },
  "prettier": {
    "importOrder": [
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "dependencies": {
    "cleye": "^1.2.0",
    "dotenv": "16.0.1",
    "ethers": "5.6.8",
    "loglevel": "1.8.0",
    "web3": "1.7.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "16.11.36",
    "esno": "^0.16.3",
    "husky": "8.0.1",
    "lint-staged": "12.4.2",
    "pkgroll": "^1.3.1",
    "prettier": "^2.6.2",
    "sort-package-json": "1.57.0",
    "typescript": "4.7.2",
    "xo": "0.49.0"
  }
}
